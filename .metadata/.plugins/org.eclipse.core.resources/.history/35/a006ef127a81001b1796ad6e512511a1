package collections;
import java.util.*;

public class MaxScore {
	static Scanner sc=new Scanner(System.in);
	
public static void main(String args[])
{
	
	HashMap<String, Integer> map=new HashMap<String, Integer>();
	System.out.println("Enter the number of players");
	int n=Integer.parseInt(sc.next());
	map=getValues(map, n);
	for(Map.Entry m : map.entrySet()){   
//		int temp=map.getValue(m);
//		if(max<)
//		{
//			
//		}
	    System.out.println(m.getKey()+" \n"+m.getValue());  
	}
	    sc.close();
}
static HashMap<String, Integer> getValues(HashMap<String, Integer> map,int n)
{int runs,max=0;
String name;
for(int i=0;i<n;i++)
{
	System.out.println("Enter the details of player "+ i);
name=sc.next();
	runs=Integer.parseInt(sc.next());
	map.put(name, runs);}
	
}
}